<h1>heres where I add my projects</h1>
 
<%= form_with scope: :project, url: projects_path, local: true  do |form|%>
<p>
<%= form.label :title%>
<%= form.text_field :title%>
</p>
<p>
<%= form.label :description%>
<%= form.text_area :description%>
</p>
<p>
<%= form.label :github%>
<%= form.text_field :github%>
</p>
<p>
<%=form.submit%>
</p>
<% end%>